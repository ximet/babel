{
  "name": "babel",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "make build",
    "fix": "make fix",
    "lint": "make lint",
    "precommit": "lint-staged",
    "test": "make test"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-flow": "7.0.0-beta.40",
    "@babel/preset-stage-0": "7.0.0-beta.40",
    "@babel/register": "7.0.0-beta.40",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.3.0-0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-charcodes": "^0.1.0",
    "babylon": "7.0.0-beta.40",
    "browserify": "^13.1.1",
    "bundle-collapser": "^1.2.1",
    "chai": "^4.1.0",
    "chalk": "^2.0.0",
    "charcodes": "^0.1.0",
    "derequire": "^2.0.2",
    "enhanced-resolve": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-babel": "^7.0.2",
    "eslint-plugin-flowtype": "^2.20.0",
    "eslint-plugin-prettier": "^2.5.0",
    "flow-bin": "^0.66.0",
    "graceful-fs": "^4.1.11",
    "gulp": "^3.9.0",
    "gulp-babel": "^8.0.0-beta.0",
    "gulp-filter": "^5.1.0",
    "gulp-newer": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "husky": "^0.14.3",
    "jest": "^21.3.0-0",
    "lerna": "2.0.0",
    "lerna-changelog": "^0.5.0",
    "lint-staged": "^6.0.1",
    "lodash": "^4.2.0",
    "merge-stream": "^1.0.1",
    "output-file-sync": "^2.0.0",
    "prettier": "1.10.2",
    "pump": "^1.0.2",
    "rimraf": "^2.4.3",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-stream": "^1.24.1",
    "through2": "^2.0.0",
    "uglify-js": "^2.4.16",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^3.4.1",
    "webpack-dependency-suite": "^2.4.4",
    "webpack-stream": "^4.0.0"
  },
  "engines": {
    "node": ">= 4.x <= 9.x",
    "npm": ">= 2.x <= 5.x",
    "yarn": ">=0.27.5 || >=1.0.0-20170811"
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=codeframe --rulesdir='./scripts/eslint_rules'"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.js"
    ],
    "testRegex": "./packages/[^/]+/test/.+\\.js$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/",
      "/test/tmp/",
      "/test/__data__/",
      "/test/helpers/",
      "<rootDir>/test/warning.js",
      "_browser.js"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/testSetupFile.js",
    "transformIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/",
      "/test/tmp/",
      "/test/__data__/"
    ],
    "modulePaths":[
      "<rootDir>/packages/"
    ],
    "modulePathIgnorePatterns": [
      "/test/fixtures/"
    ]
  }
}
